{
  "name": "lmarttins/php-kafka-consumer",
  "description": "A consumer of Kafka in PHP",
  "keywords": [
    "php",
    "kafka",
    "consumer"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "monolog/monolog": "^1.24",
    "illuminate/console": "6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Kafka\\Consumer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kafka\\Consumer\\Laravel\\Providers\\PhpKafkaConsumerProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
